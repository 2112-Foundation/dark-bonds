  "accounts": [
    {
      "name": "Bond",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "owner",
            "docs": ["Public key of the owner that can sell/split this bond."],
            "type": "publicKey"
          },
          {
            "name": "idx",
            "docs": [
              "Index of the bonc_counter at the ibo state account at the instance this bond was created."
            ],
            "type": "u32"
          },
          {
            "name": "swapPrice",
            "docs": [
              "Swap: if non zero someone can exucte it with a transfer otherwise not for sale. Exchange using underlying liqudiity token."
            ],
            "type": "u64"
          },
          {
            "name": "totalClaimable",
            "docs": ["Total amount that can be made at maturity."],
            "type": "u64"
          },
          {
            "name": "maturityDate",
            "docs": [
              "Data in seconds since the epoch when full bond value can be withdrawn."
            ],
            "type": "i64"
          },
          {
            "name": "lastClaimed",
            "docs": ["Last redemption date in seconds since the epoch."],
            "type": "i64"
          },
          {
            "name": "bondStart",
            "docs": ["Bond creation date in seconds since the epoch."],
            "type": "i64"
          },
          {
            "name": "matureOnly",
            "docs": ["Redemption possible only once full maturity is reached."],
            "type": "bool"
          }
        ]
      }
    },
    {
      "name": "Gate",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "gateSettings",
            "docs": ["Type of gate_settings."],
            "type": {
              "vec": {
                "defined": "GateType"
              }
            }
          }
        ]
      }
    },
    {
      "name": "Ibo",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "lockupsLocked",
            "docs": [
              "After being set to true, IBO admin can't add further lock-ups."
            ],
            "type": "bool"
          },
          {
            "name": "withdrawsLocked",
            "docs": [
              "After being set to true, IBO admin can't withdraw underlying token until end of the IBO set by end_date"
            ],
            "type": "bool"
          },
          {
            "name": "fixedExchangeRate",
            "docs": [
              "Fixed rate of conversion between underlying token and liquidity coin.\n        Set by the IBO deployers at the initalisation of the IBO.\n        Allows to bypass innability to add jupiter."
            ],
            "type": "u64"
          },
          {
            "name": "swapCut",
            "type": "u64"
          },
          {
            "name": "liveDate",
            "docs": ["Date after which bonds can be purchased."],
            "type": "i64"
          },
          {
            "name": "endDate",
            "docs": ["Date after which bonds can not be purchased."],
            "type": "i64"
          },
          {
            "name": "liquidityToken",
            "docs": ["Mint of the token in which bonds will be redeemed."],
            "type": "publicKey"
          },
          {
            "name": "underlyingToken",
            "docs": ["Mint of the token which will be used to purchase bonds."],
            "type": "publicKey"
          },
          {
            "name": "recipientAddress",
            "docs": ["Receives a cut of the provided liquidity token amount."],
            "type": "publicKey"
          },
          {
            "name": "admin",
            "docs": [
              "Admin of the ibo which can:\n     - add/remove gates\n     - add/remove lockups\n     - change exchanged rate"
            ],
            "type": "publicKey"
          },
          {
            "name": "lockupCounter",
            "docs": [
              "Total number of indidividual bond option types created for this IBO."
            ],
            "type": "u32"
          },
          {
            "name": "bondCounter",
            "docs": ["Total number of bonds issued under this IBO."],
            "type": "u32"
          },
          {
            "name": "gateCounter",
            "docs": [
              "Total number of gates which restrict which lock up option can be used to purchase the bond."
            ],
            "type": "u32"
          },
          {
            "name": "nftCounter",
            "type": "u32"
          },
          {
            "name": "nftBasePrice",
            "type": "u64"
          },
          {
            "name": "treeCounter",
            "type": "u8"
          }
        ]
      }
    },
    {
      "name": "Lockup",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "period",
            "docs": ["Minimum lockup period in seconds."],
            "type": "i64"
          },
          {
            "name": "apy",
            "docs": ["Yearly APY for this lockup."],
            "type": "i64"
          },
          {
            "name": "gates",
            "docs": [
              "Pointers to the gates that will allow this lock up to be used."
            ],
            "type": {
              "vec": "u32"
            }
          },
          {
            "name": "matureOnly",
            "docs": ["Can only withdraw all at once at the end."],
            "type": "bool"
          },
          {
            "name": "limit",
            "docs": ["Total amount to be sold under this lock-up option."],
            "type": "u64"
          },
          {
            "name": "purchasePeriod",
            "docs": [
